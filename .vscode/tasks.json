{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-simple",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Minio.Examples.Simple/Minio.Examples.Simple.csproj",
                "-c", "Debug",
                "-f", "net10.0",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build-host",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Minio.Examples.Host/Minio.Examples.Host.csproj",
                "-c", "Debug",
                "-f", "net10.0",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}
